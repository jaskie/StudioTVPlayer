<?xml version="1.0" encoding="utf-8" ?>
<PresetList>
  <EncoderPreset PresetName="XDCAM HD422 50Mbps"
                 VideoCodec="mpeg2video"
                 VideoBitrate="50000"
                 AudioCodec="pcm_s24le"
                 Options="maxrate=50000k,bufsize=3835k,minrate=50000k,flags=+ildct+ilme"
                 FilenameExtension="mxf"
                 PixelFormat="yuv422p"
                 >
    <Description>Popular HD production format, 50 Mbps video, PCM audio.
Creates MXF file.</Description>
  </EncoderPreset>
  <EncoderPreset PresetName="H.264 4Mbps"
                 VideoCodec="libx264"
                 VideoBitrate="4096"
                 AudioCodec="aac"
                 AudioBitrate="128"
                 Options="preset=veryfast,g=30"
                 FilenameExtension="mov"
                 >
    <Description>Preview quality, 4 Mbps H.264 CPU-encoded video, 128 kbps audio.
Creates MOV file.</Description>
  </EncoderPreset>
</PresetList>