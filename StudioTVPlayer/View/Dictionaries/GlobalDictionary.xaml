                    xmlns:System="clr-namespace:System;assembly=mscorlib">

    <SolidColorBrush x:Key="Program_MainColor" Color="#262626" />
    <SolidColorBrush x:Key="Program_SecondaryColor" Color="#0D0D0D" />
    <SolidColorBrush x:Key="Program_Foreground" Color="LightGray" />
    <SolidColorBrush x:Key="Program_MouseOver" Color="#50262626" />

    <SolidColorBrush x:Key="Player_Foreground" Color="White" />
    <SolidColorBrush x:Key="Player_Duration" Color="Green" />
    <SolidColorBrush x:Key="Player_Out" Color="Red" />

    <SolidColorBrush x:Key="Player_PlayEllipseBackground" Color="Transparent" />
    <SolidColorBrush x:Key="Player_StopEllipseBackground" Color="Transparent" />
    <SolidColorBrush x:Key="Player_PauseEllipseBackground" Color="Transparent" />
    <SolidColorBrush x:Key="Player_NextEllipseBackground" Color="Transparent" />
    <SolidColorBrush x:Key="Player_PlayEllipseStroke" Color="Green" />
    <SolidColorBrush x:Key="Player_PauseEllipseStroke" Color="Yellow" />
    <SolidColorBrush x:Key="Player_StopEllipseStroke" Color="Red" />
    <SolidColorBrush x:Key="Player_NextEllipseStroke" Color="Blue" />

    <SolidColorBrush x:Key="Player_PlayShape" Color="Green" />
    <SolidColorBrush x:Key="Player_PauseShape" Color="Yellow" />
    <SolidColorBrush x:Key="Player_StopShape" Color="Red" />
    <SolidColorBrush x:Key="Player_NextShape" Color="Blue" />

    <SolidColorBrush x:Key="Player_PlayEllipseMouseOver" Color="#2000FF00" />
    <SolidColorBrush x:Key="Player_StopEllipseMouseOver" Color="#20FF0000" />
    <SolidColorBrush x:Key="Player_PauseEllipseMouseOver" Color="#20FFFF00" />
    <SolidColorBrush x:Key="Player_NextEllipseMouseOver" Color="#400000FF" />

    <SolidColorBrush x:Key="Player_ItemBackground" Color="#FF404040" />

    <Style x:Key="ContextMenuIconless" TargetType="ContextMenu">
        <Setter Property="Background" Value="{StaticResource Program_SecondaryColor}" />
        <Setter Property="Foreground" Value="{StaticResource Program_Foreground}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ContextMenu">
                    <Border Background="{TemplateBinding Background}" BorderBrush="{StaticResource Program_Foreground}" BorderThickness="{TemplateBinding BorderThickness}">
                        <StackPanel ClipToBounds="True" Orientation="Horizontal" IsItemsHost="True" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="BrowserFocused" TargetType="{x:Type UserControl}">
        <Style.Triggers>
            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                <Setter Property="BorderBrush" Value="LightGray" />
                <Setter Property="BorderThickness" Value="2" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="PlayerFocused" TargetType="{x:Type UserControl}">
        <Style.Triggers>
            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                <Setter Property="BorderBrush" Value="Red" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="ListItem_FocusVisualStyle" TargetType="Control">
        <Setter Property="FocusManager.FocusedElement" Value="{Binding RelativeSource={RelativeSource Self}}"></Setter>
    </Style>
    
